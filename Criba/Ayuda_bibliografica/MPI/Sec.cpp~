# include <math.h>
# include <mpi.h>
# include <stdio.h>
# include <stdlib.h>
# include <time.h>

int main ( int argc, char *argv[] );
int prime_number ( int n);

int main ( int argc, char *argv[] )
{
  int i,id,ierr,n,n_factor,n_hi,n_lo,p,primes,primes_part,tam;
  double wtime;

  n_lo = 1;
  n_hi = 48615;
  n_factor = 2;

 
 	wtime = MPI_Wtime ( );
  	n = 2;
	tam = sqrt(n_hi);

  while ( n <= tam )
  {
    primes_part = prime_number ( n );
    n++;
  }

  wtime = MPI_Wtime ( ) - wtime ; 
  printf("Tiempo: %f", wtime);

  return 0;


	
}

int prime_number ( int n)

{
  int i;
  int j;
  int prime;
  int total;

  total = 0;

  for ( i = 2 ; i <= n; i++ )
  {
    prime = 1;
    for ( j = 2; j < i; j++ )
    {
      if ( ( i % j ) == 0 )
      {
        prime = 0;
        break;
      }
    }
    total = total + prime;
  }
  return total;
}
